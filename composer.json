{
  "name": "tajawal/app-boilerplate",
  "license": "proprietary",
  "version": "1.0.0",
  "require": {
    "php": ">=7.0",
    "slim/slim": "~3",
    "catfan/Medoo": "~1",
    "symfony/yaml": "~4",
    "vlucas/valitron": "~1",
    "ramsey/uuid": "~3",
    "robmorgan/phinx": "~0",
    "lcobucci/jwt": "~3",
    "swiftmailer/swiftmailer": "~6",
    "monolog/monolog": "~1",
    "nesbot/carbon": "^1.22",
    "firebase/php-jwt": "^5.0",
    "tuupola/slim-jwt-auth": "^2.3",
    "meabed/mongoose-php": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~6",
    "fzaninotto/faker": "~1"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "app/tests/"
    }
  },
  "repositories": [
    {
      "url": "https://github.com/Meabed/mongoose-php",
      "type": "git"
    }
  ],
  "scripts": {
    "start": "php -S 0.0.0.0:3000 -t public public/index.php",
    "test": "./phpunit"
  }
}
